{
  "name": "nafsi-web-sdk",
  "version": "1.0.0",
  "description": "Nafsi Identity Verification Web SDK",
  "main": "dist/nafsi.js",
  "scripts": {
    "build": "webpack",
    "postbuild": "bash setup-public.sh",
    "dev": "webpack --watch --mode development",
    "serve": "python3 -m http.server 8000",
    "start": "node server.js",
    "cdn": "npm run build && npm start",
    "docker:build": "npm run build && docker build -t nafsi-web-sdk .",
    "docker:run": "docker run -d -p 4413:4413 --name nafsi-web-sdk nafsi-web-sdk",
    "docker:dev": "docker-compose -f docker-compose.dev.yml up --build",
    "docker:prod": "npm run build && docker-compose up -d",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "nafsi",
    "identity",
    "verification",
    "kyc",
    "sdk"
  ],
  "author": "Nafsi AI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/nafsi-ai/nafsi-web-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/nafsi-ai/nafsi-web-sdk/issues"
  },
  "homepage": "https://nafsi.ai",
  "files": [
    "dist/",
    "README.md",
    "LICENSE"
  ],
  "devDependencies": {
    "@babel/core": "^7.23.0",
    "@babel/preset-env": "^7.23.0",
    "babel-loader": "^9.1.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5"
  }
}
